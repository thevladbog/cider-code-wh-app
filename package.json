{
  "name": "bottle-code-wh-app",
  "productName": "bottle-code-wh-app",
  "version": "1.0.2",
  "description": "My Electron application description",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "vite --port 5173",
    "dev:electron": "cross-env NODE_ENV=development electron-forge start",
    "rebuild": "npx @electron/rebuild",
    "postinstall": "npx @electron/rebuild",
    "dev:all": "concurrently -n vite,electron \"npm run dev\" \"npx wait-on http://localhost:5173 && npm run dev:electron\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --ext .ts,.tsx . --fix",
    "build": "vite build && electron-forge make",
    "run-app": "powershell -File .\\run-dev.ps1",
    "build:beta": "node ./scripts/version.cjs && npm run build",
    "build:release": "node ./scripts/version.cjs && npm run build",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:scripts": "vitest run tests/scripts",
    "test:api": "vitest run tests/services",
    "test:filter": "node scripts/run-tests.js",
    "test:components": "vitest run tests/components",
    "test:ui": "vitest --ui",
    "test:update": "vitest run --update",
    "test:models": "vitest run tests/models",
    "test:store": "vitest run tests/store",
    "test:utils": "vitest run tests/utils",
    "test:integration": "vitest run tests/integration",
    "certs:generate-dev": "node scripts/generate-dev-certs.cjs",
    "certs:generate-letsencrypt": "node scripts/generate-letsencrypt-http.cjs",
    "certs:renew": "node scripts/renew-certificates.cjs",
    "certs:setup-auto-renewal": "node scripts/setup-auto-renewal.cjs",
    "certs:load-local": "set SECRET_PROVIDER=local && node scripts/load-certificates.cjs",
    "certs:load-letsencrypt": "set SECRET_PROVIDER=letsencrypt && node scripts/load-certificates.cjs"
  },
  "keywords": [],
  "author": {
    "name": "Vladislav Bogatyrev",
    "email": "vladislav.bogatyrev@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-react": "^7.27.1",
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^4.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^22.15.30",
    "@types/node-forge": "^1.3.11",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.5.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "electron": "36.3.2",
    "electron-is-dev": "^3.0.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "jsdom": "^24.1.3",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^5.4.19",
    "vitest": "^3.1.4",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@tanstack/react-query": "^4.29.0",
    "date-fns": "^4.1.0",
    "electron-squirrel-startup": "^1.0.1",
    "heroicons": "^2.2.0",
    "node-fetch": "^3.3.2",
    "node-forge": "^1.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.3.0"
  }
}